{
  "source": {
    "include": ["preview-lib"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "sourceType": "module",
  "opts": {
    "destination": "./preview",
    "encoding": "utf8",
    "recurse": true,
    "template": ".",
    "tutorials": "preview-lib/tutorials"
  },
  "templates": {
    "default": {
      "includeDate": false
    },
    "kindLabels": {
      "tutorial": "guide"
    },
    "nav": {
      "order": ["module", "class", "tutorial", "interface", "mixin", "namespace", "method", "member", "typedef", "enum", "event"]
    },
    "javascripts": [
      "./preview-assets/scripts/custom.js"
    ],
    "stylesheets": [
      "./preview-assets/styles/custom.css"
    ]
  }
}
